{
  "name": "guido",
  "version": "3.1.6",
  "description": "Static build tool, base on webpack3",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zuzucheFE/guido.git"
  },
  "bugs": {
    "url": "https://github.com/zuzucheFE/guido/issues"
  },
  "keywords": [
    "guido",
    "webpack",
    "framework",
    "tools",
    "frontend"
  ],
  "scripts": {
    "test": "./node_modules/.bin/mocha test/index.js",
    "test:cover": "./node_modules/.bin/istanbul cover ./node_modules/mocha/bin/_mocha test/index.js -- -R spec",
    "travis:test": "npm run cover",
    "cover": "./node_modules/.bin/istanbul cover ./node_modules/mocha/bin/_mocha test/index.js --report lcovonly -- -R spec && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js && rm -rf ./coverage"
  },
  "license": "MIT",
  "bin": {
    "guido": "./bin/guido.js"
  },
  "engines": {
    "node": ">=4.3.0 <5.0.0 || >=5.10"
  },
  "main": "lib/guido.js",
  "devDependencies": {
    "chai": "^4.1.2",
    "coveralls": "^2.13.3",
    "istanbul": "^0.4.5",
    "mocha": "^3.5.3",
    "mocha-lcov-reporter": "^1.3.0",
    "postcss-devtools": "^1.1.1",
    "react-hot-loader": "^3.0.0-beta.7"
  },
  "dependencies": {
    "autoprefixer": "^7.1.5",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.1",
    "babel-loader": "^7.1.2",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-1": "^6.24.1",
    "bluebird": "^3.5.1",
    "chalk": "^2.1.0",
    "commander": "^2.11.0",
    "css-loader": "^0.28.7",
    "del": "^3.0.0",
    "eslint": "^4.8.0",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-react": "^7.4.0",
    "extend": "^3.0.1",
    "extract-text-webpack-plugin": "^3.0.1",
    "file-loader": "^1.1.5",
    "glob": "^7.1.2",
    "handlebars": "^4.0.10",
    "handlebars-loader": "^1.6.0",
    "happypack": "^4.0.0",
    "html-webpack-plugin": "^2.30.1",
    "image-webpack-loader": "^3.4.2",
    "node-sass": "^4.5.3",
    "postcss-iconfont": "^1.0.0",
    "postcss-loader": "^2.0.6",
    "postcss-sprites": "^4.2.1",
    "prompt": "^1.0.0",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.18.2",
    "url-loader": "^0.5.9",
    "webpack": "^3.6.0",
    "webpack-dev-server": "^2.9.1",
    "webpack-parallel-uglify-plugin": "^1.0.0"
  }
}
